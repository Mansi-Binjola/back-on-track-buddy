{
    "manifest_version": 2,
    "name": "Back To Track Buddy",
    "version": "1.0.0",
    "description": " A simple extension which gurantees to get back to track",
    "icons" : {
        "128": "icons/icon128.png",
        "48": "icons/icon48.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon" : "icons/icon128.png",
        "default_title": "Back To Track Buddy"
    },
    "background": {
        "scripts":["jquery-3.6.0.min.js", "content.js"],
        "persistent": false
    },
    "permissions": [
        "storage",
        "notifications",
        "contextMenus",
        "tabs"
    ]
    
}