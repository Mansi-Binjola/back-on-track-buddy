{
    "manifest_version": 2,
    "name": "Back To Track Buddy",
    "version": "1.0.0",
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Back To Track Buddy"
    },
    "permissions": ["storage",
        "<all_urls>",
        "activeTab"],
    "content_scripts":[ {
        "matches":["*://*/*"],
        "js":["content.js"]
    }]
}